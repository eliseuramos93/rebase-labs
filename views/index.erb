<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/style.css">
    <title>Rebase Labs</title>
</head>
<body>
    <section id="test-results">
        <h2>Resultados dos Exames</h2>
        <div class="table-div">
            <table id="test-results-table">
                <thead>
                    <tr id="table-headers">
                        <th>Token do exame</th>
                        <th>Data do resultado</th>
                        <th>Nome completo</th>
                        <th>CPF</th>
                        <th>E-mail</th>
                        <th>Data de Nascimento (AAAA-MM-DD)</th>
                        <th>CRM Médico(a)</th>
                        <th>Estado do CRM</th>
                        <th>Nome do médico(a)</th>
                    </tr>
                </thead>
                <tbody id="tbody">
                </tbody>
            </table>
        </div>
        <div id="pagination"></div>
    </section>
    <script>
        <% if ENV['APP_ENV'] == 'test' %>
            const tests_url = 'http://127.0.0.1:4242/tests'
        <% else %>
            const tests_url = 'http://localhost:3000/tests'
        <% end %>
    </script>
    <script src="main.js"></script>
</body>
</html>